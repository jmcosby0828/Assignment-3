<h2>Books by Zora Neale Hurston</h2>
<table>
    <thead>
        <tr>
            <th>Title</th>
            <th>Price</th>
            <th>Cover</th>
        </tr>
    </thead>
    
    <tbody>
        {{#each books}}
        <tr>
            <td>{{this.title}}</td>
            <td>${{this.price}}</td>
            <td>
                <img src = "/images/{{this.image}}" alt = "Cover of {{this.title}}">
            </td>
        </tr>
        {{/each}}
    </tbody>
</table>